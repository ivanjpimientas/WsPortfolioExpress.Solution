<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Portfolio Express.Web</title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Datatables bs4-->
    <link rel="stylesheet" href="~/assets//plugins/datatables-bs4/css/dataTables.bootstrap4.min.css" />
    <link rel="stylesheet" href="~/assets//plugins/datatables-responsive/css/responsive.bootstrap4.min.css" />
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/assets/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/assets/dist/css/adminlte.min.css">
    <!-- Overlay Scrollbars css-->
    <link rel="stylesheet" href="~/assets/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- CodeMirror -->
    <link rel="stylesheet" href="~/assets/plugins/codemirror/codemirror.css">
    <link rel="stylesheet" href="~/assets/plugins/codemirror/theme/monokai.css">
    <!-- Simple MDE css-->
    <link rel="stylesheet" href="~/assets/plugins/simplemde/simplemde.min.css">
    @RenderSection("Styles", required: false)
</head>
<body class="hold-transition sidebar-mini dark-mode">
    <div class="wrapper">
        <partial name="_Header.cshtml" />

        <div class="content-wrapper">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <partial name="_Aside.cshtml" />

        <partial name="_Footer.cshtml" />
    </div>

    <!-- Insert this line above script imports  -->
    <script>if (typeof module === 'object') { window.module = module; module = undefined; }</script>

    <!-- jQuery -->
    <script src="~/assets/plugins/jquery/jquery.min.js" crossorigin="anonymous"></script>
    <!-- Bootstrap 4 -->
    <script src="~/assets/plugins/bootstrap/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>

    <!-- Datatables -->
    <script src="~/assets/plugins/datatables/jquery.dataTables.min.js" crossorigin="anonymous"></script>
    <script src="~/assets/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
    <script src="~/assets/plugins/datatables-responsive/js/dataTables.responsive.min.js" crossorigin="anonymous"></script>
    <script src="~/assets/plugins/datatables-responsive/js/responsive.bootstrap4.min.js" crossorigin="anonymous"></script>

    <!-- Sweetalert -->
    <script src="~/assets/plugins/sweetalert2/sweetalert2.all.js" crossorigin="anonymous"></script>
    <!-- AdminLTE App -->
    <script src="~/assets/dist/js/adminlte.min.js" crossorigin="anonymous"></script>
    <!-- Overlay Scrollbars js-->
    <script src="~/assets/plugins/overlayScrollbars/js/OverlayScrollbars.min.js" crossorigin="anonymous"></script>
    <!-- CodeMirror -->
    <script src="~/assets/plugins/codemirror/codemirror.js"></script>
    <script src="~/assets/plugins/codemirror/mode/css/css.js"></script>
    <script src="~/assets/plugins/codemirror/mode/xml/xml.js"></script>
    <script src="~/assets/plugins/codemirror/mode/htmlmixed/htmlmixed.js"></script>
    <script src="~/assets/plugins/codemirror/mode/javascript/javascript.js"></script>
    <!-- Simple MDE js-->
    <script src="~/assets/plugins/simplemde/simplemde.min.js" crossorigin="anonymous"></script>
    <!-- Customers script -->
    <script src="~/scripts/portfolioexpress.customers.js" crossorigin="anonymous"></script>
    <!-- Insert this line after script imports -->
    <script>if (window.module) module = window.module;</script>

    @await RenderSectionAsync("Scripts", required: false)

    @await Html.PartialAsync("_NotificationPanel")
</body>
</html>
